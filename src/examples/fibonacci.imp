let var fibN = 0, var an = 0, var i = 1 
in
    let fn fib(n) = 
        while (i <= n) do 
            if (i == 1) then
                fibN := 1 
                an := 0
            else 
                fibN := fibN + an
                an := fibN - an
            end
            i := i + 1
        end
    in fib(6)
end
